#include<stdio.h>
#include<conio.h>
#define max 10
 int main()
  {
   int a[max],n,i;flag=0;
   printf("enter the limit less than %d",max);
   scanf("%d",&n);
   for(i=0;i<n;i++)
    scanf("%d",&a[i]);
  for(i=0;i<n;i++)
  {
   if(a[abs(a[i])]>0 && a[abs(a[i])]!=n)
    a[abs(a[i])]=-1*abs(a[i]);
   else if(a[abs(a[i])]<0 || a[abs(a[i])]==n)
    {
     printf("yes");
     flag=1;
     break;
     }
     else
     a[abs(a[i])]=n;
    }
  if(flag==0)
  printf("no");
  return 1;
}
int abs(int a)
{
 if(a>0)
 return a;
 else
 return a*-1;
 }
 
